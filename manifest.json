{
  "manifest_version": 2,
  "name": "Open media in new tab - android",
  "version": "1.0",
  "description": "Adds a download button to any video on the page.",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
